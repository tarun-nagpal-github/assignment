# Copy to secret.yaml and fill in values. Do not commit secret.yaml.
# kubectl create secret generic companysearch-opensearch --from-literal=OPENSEARCH_INITIAL_ADMIN_PASSWORD=<password> -n companysearch --dry-run=client -o yaml > secret.yaml
apiVersion: v1
kind: Secret
metadata:
  name: companysearch-opensearch
  namespace: companysearch
type: Opaque
stringData:
  OPENSEARCH_INITIAL_ADMIN_PASSWORD: "<replace-with-your-password>"
